<Window x:Class="Binding.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Binding"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <StackPanel Height="50" DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Margin="5" Width="50" Content="Add"/>
            <Button Margin="5" Width="50" Content="Remove"/>
            <Button Margin="5" Width="50" Content="Change"/>
        </StackPanel>
        <ListBox ItemsSource="{Binding listaImena}" Name="lstBox"></ListBox>
    </DockPanel>
</Window>
