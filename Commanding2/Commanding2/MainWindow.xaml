<Window x:Class="Commanding2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Commanding2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <TextBox Name="bla" DockPanel.Dock="Top" Text="{Binding Unos}" Margin="14,0,-14,0"></TextBox>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" DockPanel.Dock="Bottom">
            <Button Content="Add" Command="{Binding DodajKomanda}"
                    CommandParameter="{Binding .}"/>
            <Button Content="Remove"/>
        </StackPanel>
        <ListBox ItemsSource="{Binding Lista}"></ListBox>
    </DockPanel>
        
    
</Window>
